{
  "name": "ebay-api-fortress",
  "version": "1.0.0",
  "description": "Prime-eBay CLI tooling for eBay API automation and experiments.",
  "type": "module",
  "main": "index.js",
  "bin": {
    "prime-ebay": "index.js"
  },
  "scripts": {
    "start": "node index.js",
    "build": "tsc",
    "dev": "node --watch index.js",
    "api": "node src/routes/server.js",
    "tunnel": "node tunnel/server.mjs",
    "doctor": "node scripts/doctor.mjs",
    "opencode:codex-auth:check": "node scripts/opencode-codex-auth.mjs check",
    "opencode:codex-auth:update": "node scripts/opencode-codex-auth.mjs update --write",
    "opencode:codex-auth:update:dry": "node scripts/opencode-codex-auth.mjs update",
    "opencode:codex-auth:refresh-cache": "node scripts/opencode-codex-auth.mjs refresh-cache",
    "opencode:supermemory:check": "node scripts/opencode-supermemory.mjs check",
    "opencode:supermemory:update": "node scripts/opencode-supermemory.mjs update --write",
    "opencode:supermemory:update:dry": "node scripts/opencode-supermemory.mjs update",
    "opencode:supermemory:refresh-cache": "node scripts/opencode-supermemory.mjs refresh-cache",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "test": "vitest run",
    "test:watch": "vitest",
    "format": "prettier --check .",
    "format:fix": "prettier --write .",
    "tsc": "tsc --noEmit",
    "check": "npm run lint && npm test"
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "axios": "^1.13.2",
    "csv-parser": "^3.2.0",
    "dotenv": "^17.2.3",
    "fastify": "^5.6.2",
    "readline": "^1.3.0",
    "zod": "^4.1.13"
  },
  "devDependencies": {
    "@eslint/js": "^8.57.0",
    "@types/node": "^25.0.3",
    "eslint": "^8.57.0",
    "prettier": "^3.3.3",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3",
    "vitest": "^2.1.4"
  },
  "directories": {
    "test": "tests"
  }
}
